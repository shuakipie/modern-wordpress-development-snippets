{
  "Generate block.json file": {
    "prefix": [
      "rw",
      "wp",
      "json",
      "block"
    ],
    "description": "Generates the contents of a block.json file",
    "body": [
      "{\r",
      "    \"apiVersion\": 2,\r",
      "    \"name\": \"${1:example-name}\",\r",
      "    \"title\": \"${2:Title}\",\r",
      "    \"category\": \"${3:common}\",\r",
      "    \"description\": \"${4:Description}\",\r",
      "    \"text-domain\": \"${5:example-text-domain}\",\r",
      "    \"icon\": \"${6:feedback}\",\r",
      "    \"attributes\": {},\r",
      "    \"keywords\": [],\r",
      "    \"version\": \"${7:0.0.1}\",\r",
      "    \"providesContext\": {},\r",
      "    \"usesContext\": [],\r",
      "    \"styles\": [],\r",
      "    \"example\": {\r",
      "        \"attributes\": {}\r",
      "    },\r",
      "    \"supports\": {},\r",
      "    \"editorScript\": \"${8:file:./build/index.js}\",\r",
      "    \"script\": \"${9:file:./build/script.js}\",\r",
      "    \"editorStyle\": \"${10:file:./build/index.css}\",\r",
      "    \"style\": \"${11:file:./build/index.css}\",\r",
      "\t\"viewScript\" : \"${12:file:./build/view.js}\",\r",
      "\t\"render\" : \"${13:file:./build/template.php}\"\r",
      "}"
    ]
  },
  "Generate a basic theme.json file": {
    "prefix": [
      "rw",
      "wp",
      "json",
      "theme"
    ],
    "description": "Generates the contents of a theme.json file",
    "body": [
      "{\r",
      "\t\"$schema\": \"http://schemas.wp.org/trunk/theme.json\",\r",
      "\t\"version\": 2,\r",
      "\t\"settings\": {\r",
      "\t\t\"layout\": {\r",
      "\t\t\t\"contentSize\": \"${1:650px}\",\r",
      "\t\t\t\"wideSize\": \"${2:1000px}\"\r",
      "\t\t}\r",
      "\t}\r",
      "}"
    ]
  }
}